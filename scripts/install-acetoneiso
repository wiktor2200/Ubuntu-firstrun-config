# install-acetoneiso
# Utilties - Install ISO images tool
# Install ISO images tool, mount, unmount, modify. - acetoneiso
echo -e "Running: Utilties - Install ISO images tool - ./scripts/install-acetoneiso"

sudo apt-get install -y acetoneiso

if [ $? -eq 0 ]; then
   echo -e "\\nOK\\n"
   exit 0
else
   echo -e "\\nFAIL\\n"
   exit 1
fi
