# install-slack
# Useful - Install Slack
# Install Slack team collaboration tool from snap package.
echo -e "Running: Useful - Install Slack - ./scripts/install-slack"

sudo snap install slack --classic

if [ $? -eq 0 ]; then
   echo -e "\\nOK\\n"
   exit 0
else
   echo -e "\\nFAIL\\n"
   exit 1
fi
