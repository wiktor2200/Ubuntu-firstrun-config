# install-hex-bless-editor
# IDE - Install Powerful HEX editor – bless
# Install hex editor - bless - more powerful, but needs more dependencies. – bless.
echo -e "Running: IDE - Install Powerful HEX editor – bless - ./scripts/install-hex-bless-editor"

sudo apt-get install -y bless

if [ $? -eq 0 ]; then
   echo -e "\\nOK\\n"
else
   echo -e "\\nFAIL\\n"
fi
