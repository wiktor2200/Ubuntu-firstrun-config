# install-tellico
# Useful - Install Tellico – collections manager
# Install Tellico – collections manager – books, movies, coins etc.
echo -e "Running: Useful - Install Tellico – collections manager - ./scripts/install-tellico"

# http://tellico-project.org/
sudo apt-get install -y tellico

if [ $? -eq 0 ]; then
   echo -e "\\nOK\\n"
   exit 1
else
   echo -e "\\nFAIL\\n"
   exit 0
fi
