# install-synaptic
# Utilties - Install Synaptic package manager
# Install Synaptic package manager
echo -e "Running: Utilties - Install Synaptic package manager - ./scripts/install-synaptic"

sudo apt-get install -y synaptic

if [ $? -eq 0 ]; then
   echo -e "\\nOK\\n"
else
   echo -e "\\nFAIL\\n"
fi
