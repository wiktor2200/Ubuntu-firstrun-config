# install-chromium
# Useful - Install Chromium browser
# Install open-source Chromium browser. Open-source Chrome.
echo -e "Running: Useful - Install Chromium browser - ./scripts/install-chromium"

sudo apt-get install -y chromium-browser

if [ $? -eq 0 ]; then
   echo -e "\\nOK\\n"
   exit 1
else
   echo -e "\\nFAIL\\n"
   exit 0
fi
