# install-cinnamon
# System - Install Cinnamon desktop manager
# Install Cinnamon desktop manager
echo -e "Running: System - Install Cinnamon desktop manager - ./scripts/install-cinnamon"

sudo apt-get install -y cinnamon-core

if [ $? -eq 0 ]; then
   echo -e "\\nOK\\n"
   exit 1
else
   echo -e "\\nFAIL\\n"
   exit 0
fi
