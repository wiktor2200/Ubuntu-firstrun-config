# install-hex-gnome-editor
# IDE - Install Simple HEX editor - ghex
# Install simple Gnome hex editor.
echo -e "Running: IDE - Install Simple HEX editor - ghex - ./scripts/install-hex-gnome-editor"

sudo apt-get install -y ghex

if [ $? -eq 0 ]; then
   echo -e "\\nOK\\n"
   exit 0
else
   echo -e "\\nFAIL\\n"
   exit 1
fi
