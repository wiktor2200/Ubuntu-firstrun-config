# install-gnome-tools
# Utilties - Install Gnome tools
# Install Gnome settings managers and utils.
echo -e "Running: Utilties - Install Gnome tools - ./scripts/install-gnome-tools"

sudo apt-get install -y chrome-gnome-shell gnome-calculator gnome-system-monitor gnome-system-tools gnome-tweaks gucharmap

if [ $? -eq 0 ]; then
   echo -e "\\nOK\\n"
else
   echo -e "\\nFAIL\\n"
fi
